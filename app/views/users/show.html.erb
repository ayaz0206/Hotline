

<div class="row">
  <div class="large-4 columns">
		<img src= "<%= @profile_user.image %> ">
		<h4> <strong><%= @profile_user.name %></strong></h4>
		<p> Joined on <%= @profile_user.created_at %></p>
  </div>

  <div class="large-8 columns">
  	<section class='profile-questions'>
			<h3> Questions Asked:</h3>
			<ol>
				<% @profile_user.questions.each do |question| %>
					<li><a href='<%= question_path(question) %>'><%= question.title %></a></li>
				<% end %>
			</ol>
		</section>
  </div>
</div>


