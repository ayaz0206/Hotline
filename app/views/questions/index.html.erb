
	<section class='home-feed'>
		<ul>
		<% @questions.each do |question| %>
			<li> 
				<a href ="questions/<%= question.id %>">  <%= question.title %> </a>  <br>
				<span class='username'> <%= question.user.name %> </span> <br>
				<span class='username'> Asked on: <%= question.created_at %> </span> <br>
			</li>
		<% end %> 
		</ul>
</section>